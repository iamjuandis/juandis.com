{
  "name": "juandis-portfolio",
  "version": "22.06.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "prettier": "prettier --write src/",
    "eslint": "eslint './src/**/*.{ts,tsx,js,jsx}'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,yml}": [
      "npm run prettier",
      "eslint './src/**/*.{ts,tsx,js,jsx}",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.26",
    "next": "^13.4.3",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "styled-components": "^5.3.10",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@types/gtag.js": "0.0.3",
    "@types/node": "^12.20.55",
    "@types/react": "^16.14.41",
    "@types/react-dom": "^16.9.19",
    "cross-env": "^7.0.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "typescript": "^4.9.5"
  }
}
